{
  "name": "xendit-callback",
  "version": "1.0.0",
  "description": "Backend service for handling callback from xendit (payment gateway).",
  "bin": {
    "typescript-maker": "./bin/generateApp.js"
  },
  "scripts": {
    "start": "ts-node -r reflect-metadata -r tsconfig-paths/register -r dotenv/config src/index",
    "dev": "nodemon --config restart.json",
    "clean": "rm -rf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --forceExit"
  },
  "author": "jamessaldo <jamessaldo19@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "commander": "^8.3.0",
    "express": "^4.17.1",
    "http-status": "^1.6.2",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mysql2": "^3.2.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "~2.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "~14.14.45",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "~4.28.2",
    "@typescript-eslint/parser": "~4.28.2",
    "dotenv": "^10.0.0",
    "eslint": "~7.30.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~24.3.6",
    "jest": "^27.0.6",
    "jest-html-reporter": "^3.4.1",
    "nodemon": "^2.0.19",
    "prettier": "~2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.2.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "tsutils": "~3.21.0",
    "typescript": "~4.3.5"
  },
  "main": "index.js",
  "repository": "https://github.com/jamessaldo/wedigo"
}
